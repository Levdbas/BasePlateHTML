{
    "name": "BasePlate",
    "description": "Great theme for small-medium sized projects for oldschool FTP deploment, featuring top-notch build tools.",
    "license": "MIT",
    "require": {
        "php": "^7.0"
    },
    "config": {
        "preferred-install": "dist"
    },
    "require": {
        "wpackagist-plugin/wordpress-seo": "^4.8",
        "wpackagist-plugin/custom-post-type-ui": "^1.5.3",
        "wpackagist-plugin/support-hours": "^1.3.1",
        "wpackagist-plugin/trigger-browsersync": "^0.5"
    },
    "extra": {
        "installer-paths": {
            "../../plugins/{$name}": ["type:wordpress-plugin"]
        }
    }
}
